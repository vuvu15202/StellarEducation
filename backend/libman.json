{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "library": "signalr@2.4.3",
      "destination": "wwwroot/lib/signalr/",
      "files": [
        "jquery.signalR.js",
        "jquery.signalR.min.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "microsoft-signalr@8.0.0",
      "destination": "wwwroot/microsoft-signalr/",
      "files": [
        "signalr.min.js",
        "signalr.js"
      ]
    }
  ]
}